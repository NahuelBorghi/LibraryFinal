@{
    ViewData["Title"] = "Inicio";
}
@model List<Ownership>

<h1>Index</h1>

<p>
    <a href="@Url.Action("Create", "Ownerships")">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].Book)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Book.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-controller="Ownerships" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-controller="Ownerships" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-controller="Ownerships" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
